{
  "schema_version": 1,
  "meta": {
    "name": "minimal ring",
    "author": "",
    "created": "2025-09-14"
  },
  "output": {
    "resolution": {
      "width": 1920,
      "height": 1080
    },
    "fps": 24,
    "aspect_mode": "fit",
    "profile": "youtube_1080p24"
  },
  "background": {
    "type": "gradient_dynamic",
    "colors": [
      "#0f0f1a",
      "#101a2b",
      "#192a56"
    ],
